    "/api/data-cleaning/upload": {
      "post": {
        "description": "上传Excel和CSV文件进行数据清洗处理。支持.xlsx、.xls和.csv格式，最大文件大小500MB。支持大文件流式处理。",
        "operationId": "DataCleaningController_uploadFile",
        "parameters": [],
        "requestBody": {
          "required": true,
          "description": "数据文件上传",
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary",
                    "description": "数据文件（.xlsx、.xls或.csv格式）"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "文件上传成功",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UploadResponseDto"
                }
              }
            }
          },
          "400": {
            "description": "文件格式不支持或文件过大",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDto"
                }
              }
            }
          }
        },
        "summary": "上传数据文件",
        "tags": [
          "data-cleaning"
        ]
      }
    },
    "/api/data-cleaning/status/{jobId}": {
      "get": {
        "description": "根据任务ID查询数据处理的当前状态和进度",
        "operationId": "DataCleaningController_getStatus",
        "parameters": [
          {
            "name": "jobId",
            "required": true,
            "in": "path",
            "description": "任务ID",
            "schema": {
              "example": "job_123456789",
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "成功返回处理状态",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/StatusResponseDto"
                }
              }
            }
          },
          "404": {
            "description": "任务不存在",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDto"
                }
              }
            }
          }
        },
        "summary": "查询处理状态",
        "tags": [
          "data-cleaning"
        ]
      }
    },
    "/api/data-cleaning/files": {
      "get": {
        "description": "分页查询所有上传的文件记录，支持按状态和日期范围筛选",
        "operationId": "DataCleaningController_listFiles",
        "parameters": [
          {
            "name": "page",
            "required": false,
            "in": "query",
            "description": "页码",
            "schema": {
              "minimum": 1,
              "default": 1,
              "example": 1,
              "type": "number"
            }
          },
          {
            "name": "pageSize",
            "required": false,
            "in": "query",
            "description": "每页数量",
            "schema": {
              "minimum": 1,
              "default": 10,
              "example": 10,
              "type": "number"
            }
          },
          {
            "name": "status",
            "required": false,
            "in": "query",
            "description": "文件状态筛选",
            "schema": {
              "example": "completed",
              "type": "string",
              "enum": [
                "pending",
                "processing",
                "completed",
                "failed"
              ]
            }
          },
          {
            "name": "startDate",
            "required": false,
            "in": "query",
            "description": "开始日期（ISO格式）",
            "schema": {
              "example": "2026-01-01T00:00:00.000Z",
              "type": "string"
            }
          },
          {
            "name": "endDate",
            "required": false,
            "in": "query",
            "description": "结束日期（ISO格式）",
            "schema": {
              "example": "2026-12-31T23:59:59.999Z",
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "成功返回文件列表",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FileListResponseDto"
                }
              }
            }
          }
        },
        "summary": "查询文件列表",
        "tags": [
          "data-cleaning"
        ]
      }
    },
    "/api/data-cleaning/files/{fileId}": {
